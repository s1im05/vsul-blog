{"routes":
    [
        {
            "path": "\\/404",
            "controller": "error",
            "action": "error404",
            "header": "HTTP/1.0 404 Not Found"
        },
		{
            "path": "\\/(page\\/(\\d+))?",
            "controller": "blog",
            "action": "all",
            "params": ["page_url", "page"]
        },
		{
            "path": "\\/(travel|work|games)(\\/page\\/(\\d+))?",
            "controller": "blog",
            "action": "chapter",
            "params": ["chapter", "page_url", "page"]
        },
		{
            "path": "\\/home",
            "controller": "home",
            "action": "index"
        },
		{
            "path": "\\/search\\/(.{1,255})\\/page\\/(\\d+)?",
            "controller": "search",
            "action": "search",
            "params": ["query", "page"]
        },
		{
            "path": "\\/search\\/(.{1,255})",
            "controller": "search",
            "action": "search",
            "params": ["query"]
        },
		{
            "path": "\\/logout",
            "controller": "blog",
            "action": "all"
        },
		{
            "path": "\\/rss",
            "controller": "rss",
            "action": "rss",
            "header": "Content-type: application/xhtml+xml; charset=UTF-8"
        },
		{
            "path": "\\/adm_panel",
            "controller": "admin",
            "action": "index"
        },
		{
            "path": "\\/adm_panel\\/logout",
            "controller": "admin",
            "action": "logout"
        },
		{
            "path": "\\/adm_panel\\/articles",
            "controller": "admin_articles",
            "action": "articles"
        },
		{
            "path": "\\/adm_panel\\/articles\\/ajax",
            "controller": "admin_articles",
            "action": "ajax",
            "header": "Content-type: application/json; charset=UTF-8"
        },
		{
            "path": "\\/a\\/([0-9a-z_-]{3,})",
            "controller": "blog",
            "action": "byName",
            "params": ["name"]
        },
		{
            "path": "\\/ajax\\/obs",
            "controller": "obs",
            "action": "send"
        }
		
    ]
}